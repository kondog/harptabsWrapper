<p id="notice"><%= notice %></p>

<h1>Search Result</h1>

<%= link_to 'Back', '/' %>

<p>
<table>
  <thead>
    <tr>
      <th>Harptype</th>
      <th>Songname</th>
      <th>Whosesong</th>
      <th>Genre</th>
      <th>Postedby</th>
      <th>Views</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.HarpType %></td>
        <td><%= song.SongName %></td>
        <td><%= song.WhoseSong %></td>
        <td><%= song.Genre %></td>
        <td><%= song.PostedBy %></td>
        <td><%= song.Views %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</p>
